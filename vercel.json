{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/import.js": {
            "includeFiles": "../drizzle/schema.js",
            "maxDuration": 60
        },
        "api/*.js": {
            "includeFiles": "../drizzle/schema.js"
        }
    }
}